<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="riv-shs-plugin" class="se.inera.axel.shs.broker.routing.ProductShsPluginRegistration">
        <property name="name" value="RIV-SHS-BRIDGE" />
        <property name="endpointUri" value="direct-vm:shs2riv" />
        <property name="productIds">
            <set>
                <value>00000000-0000-0000-0000-000000000001</value>
            </set>
        </property>
    </bean>

</beans>
